                "condition": "(QLA_LE1_VIS.date==QLA_LE1_VIS_LOG.date) & (QLA_LE1_VIS.time==QLA_LE1_VIS_LOG.time)"
            },
            {
                "name": "Archive_Ingestor_QLANIR",
                "inputs": "QLA_LE1_NIR,QLA_LE1_NIR_LOG",
